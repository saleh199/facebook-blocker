{
  "manifest_version": 2,

  "name": "Facebook Blocker",
  "description": "Close facebook tabs after 1 minutes",
  "version": "1.0",

  "background_page": "background.html",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}

